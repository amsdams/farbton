{
  "extends": ["tslint:recommended", "tslint-sonarts", "tslint-eslint-rules"],
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "no-console": false,
    "no-big-function": [true, 25],
    "cognitive-complexity": [true, 11],
    "prefer-template": [true, "allow-single-concat"]
  }
}
